<li class="acl-write separator"></li>
<li data-type="button" data-key="undo"><a class="grouped-left"><span class=" buttonicon buttonicon-undo" data-l10n-id="pad.toolbar.undo.title"></span></a>
</li>
<li data-type="button" data-key="redo"><a class="grouped-right"><span class=" buttonicon buttonicon-redo" data-l10n-id="pad.toolbar.redo.title"></span></a>
</li>
<li class="acl-write separator"></li>

<li id="searchinpad" class="searchinpad">
    <li class="imputs">
        <input id="padsearch" style="width: 60px; " class="padsearch" placeholder=".. find" onkeydown="if (event.keyCode == 13 || event.keyCode == 115 || event.keyCode == 118 ) return padSearchAction('S');" type="text">
    </li>
    <li class="acl-write" id="button_padsearch_up" onclick="padSearchAction('U')">
        <a class="grouped-middle" data-l10n-id="pad.toolbar.codepad_buttons.title" title="Up">
            <span class="buttonicon buttonicon-codepad_buttons_prev"></span>
        </a>
    </li>
    <li class="acl-write" id="button_padsearch_down" onclick="padSearchAction('D') ">
        <a class="grouped-right" data-l10n-id="pad.toolbar.codepad_buttons.title" title="Down F4">
            <span class="buttonicon buttonicon-codepad_buttons_next"></span>
        </a>
    </li>

    <li class="imputs">
        <input id="padreplace" style="width: 60px; align: top;" class="padreplace" placeholder=".. repl" onkeydown="if (event.keyCode == 13 || event.keyCode == 115 || event.keyCode == 118 ) return padReplaceAction();" type="text">
    </li>
    <li class="acl-write" id="button_padsearch_replace">
        <a class="grouped-right padsearch-replace" data-l10n-id="pad.toolbar.codepad_buttons.title" title="Replace F7">
            <span class="buttonicon buttonicon-codepad_buttons_replace"></span>
        </a>
    </li>

</li>


